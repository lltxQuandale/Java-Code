public class Main{
    
    
    
    public static class Chair{
        int legs;
        String wood;
        String type;
        int thickness;
        String color;
        
        public Chair(int legs, String wood, String type){
            this.legs = legs;
            this.wood = wood;
            this.type = type;
        }
        public Chair(int legs, String wood, String type, int thickness, String color){
            this.legs = legs;
            this.wood = wood;
            this.type = type;
            this.thickness = thickness;
            this.color = color;
            
        }
    }
    

    
    public static void main (String[] args){
        Chair c1 = new Chair(4,"Oak","DinnerChair" );
        Chair c2 = new Chair(4,"Oak","DinnerChair", 7, "Brown" );
        System.out.println("Chair One: ");
        System.out.println("legs: " + c1.legs);
        System.out.println("wood type: " + c1.wood);
        System.out.println("type of chair: " + c1.type);
        System.out.println("Chair Two: ");
        System.out.println("legs: " + c2.legs);
        System.out.println("wood type: " + c2.wood);
        System.out.println("Type: " + c2.type);
        System.out.println("Thickness: " + c2.thickness);
        System.out.println("Color: " + c2.color);
            
    }    
        
}
